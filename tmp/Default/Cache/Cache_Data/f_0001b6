function getPageType(){
    var pageType = 'other pages';
    if($("body").hasClass("cms-home")){
        pageType = "Home Page";
    }else if($("body").hasClass("catalog-category-view")){
        pageType = "category page";
    }else if($("body").hasClass("catalog-product-view")){
        pageType = "Product Details Page";
    }
    else if($("body").hasClass("search-result-page")){
        pageType = "Search Page";
    }            
    return pageType;
}   

function trackGoogleEvents(action,label){
    if("ga"in window){
        tracker=ga.getAll()[0];
        if(tracker){
            tracker.send("event","New Click actions",action,label);
        }
    }
}
$(document).ready(function(){  
    $(".m-pro1").click(function(ev){
        trackGoogleEvents("Medicine","");
    });

    $(".add_to_cart_fixed .cartbag").click(function(){
        trackGoogleEvents("Add to cart button","Product Holder");
    });

    $(".main-navigation .m-pro ul li a").click(function(ev){
        var label = $(this).text();
        trackGoogleEvents(label,"");
    });

    $("#search").focus(function(){
        trackGoogleEvents("Search","");
    });
    $("#upload_prescription_button").click(function(){
        trackGoogleEvents("Upload Prescription","M2");
    });
    $(".mini-cart").click(function(){
        var pageType = 'other pages';
        if($("body").hasClass("cms-home")){
            pageType = "home page";
        }
        else if($("body").hasClass("catalog-category-view")){
            pageType = "category page";
        }
        else if($("body").hasClass("catalog-product-view")){
            pageType = "product page";
        }
        trackGoogleEvents("Cart",pageType);
    });  
    $(".navigation ul li .level-top").click(function(event){
        event.stopPropagation();
        if($(this).attr("data-eventkey") == "health-library"){
            trackGoogleEvents("Health library","");
        }
        else{
            trackGoogleEvents("menu-category",$(this).text().trim());
        }
    });

    /*$(".submenu a").click(function(event){
        event.stopPropagation();
        if($(this).hasClass("health_library")){
            trackGoogleEvents("Health library","");
        }
        else{
            trackGoogleEvents("menu-sub-category",$(this).text());
        }
    }); */
});
