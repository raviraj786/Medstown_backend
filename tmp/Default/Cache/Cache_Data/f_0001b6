$(document).ready(function() {
    $('#search_form').on('submit', function(event){
        event.preventDefault();
        var str = $("#search").val().replace(/[^a-z0-9\s]/gi, ' ').replace(/[_\s]/g, ' ').toLowerCase().replace(/  +/g, ' ').trim();
        if(str==0){
            window.location.href = "https://www.netmeds.com/catalogsearch/result/url_0/all";
        }else if(str!='') {
            window.location.href = "https://www.netmeds.com/catalogsearch/result/"+str+'/all';
        } else {
            window.location.href = "https://www.netmeds.com/";
        }
    });
});
