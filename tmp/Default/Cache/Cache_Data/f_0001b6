        $("#product_request_form" ).submit(function(e) {
            $('.gx_discount_success,.gx_discount_success .pin_error,.gx_discount_success .pin_success').hide();
            e.preventDefault();
            if ($(this).valid()) {
                $('#ext_request').prop('disabled', true);
                $(".loaded").show();
                var data = $('#product_request_form').serialize();
                $.ajax({
                    url:"https://www.netmeds.com/catalog/request/searchproduct",
                    type:'POST',
                    crossDomain:true,
                    data: data,
                    start_time: new Date().getTime(),
                    success: function(response)
                    {
                        if(response == "success"){
                            $('.gx_discount_success .pin_success').text("");
                            $('#product_request_form')[0].reset(); 
                            $('#ext_request').prop('disabled', false);
                            $( "#searchprod_close" ).trigger( "click" );
                            $('.gx_discount_success .pin_success').append('Submitted successfully!');
                            $('.gx_discount_success,.gx_discount_success .pin_success').show();
                        } else {
                            $('#product_request_form')[0].reset();
                            $('#ext_request').prop('disabled', false);
                            $( "#searchprod_close" ).trigger( "click" );
                            $('.gx_discount_success,.gx_discount_success .pin_error').show();
                        } 
                    },
                    error:function(xhr, status, error){
                        var time_taken = new Date().getTime() - this.start_time;
                        genrate_log(this.url,this.start_time,new Date().getTime(),time_taken,'request_form','searchproduct',data,xhr);           
                    }
                });
            }
        });
        $("#search_mobile_no").keypress(function(event){
            evt = (event) ? event : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        });
        $.validator.addMethod(
        'Required-search-mobile', function (value) {
            if(value) return true;
            else return false;
        },'Please enter your mobile number!');
        $.validator.addMethod(
        'validate-search-mobileno',function(value) {
            return value.length > 9 &&  value.length < 11 
            && value.match(/^\d{10}$/);
        },'Please enter a valid mobile number!');
        $.validator.addMethod(
        'Required-search-product', function (value) {
            if(value) return true;
            else return false;
        },'Please enter product name!');
        $.validator.addMethod(
        'Required-search-name', function (value) {
            if(value) return true;
            else return false;
        }, "Please enter your name!");
        $.validator.addMethod("validate-search-alphaspace", function(value, element) {
            return this.optional(element) || /^[a-z\s]+$/i.test(value);
        }, "Your Name must contain only alphabets!");     
