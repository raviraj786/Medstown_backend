$(document).ready(function($) {
    $("#newsletter-validate-detail").validate({
        ignoreTitle: true,
        errorLabelContainer: '.newsletter_error'
    });
    $('.load-out').hide();
    $("#newsletter-validate-detail").submit(function(e) {
        $('.load-out').hide();
        $('.load-out').text('');
        e.preventDefault();
        e.stopImmediatePropagation();
        if ($(this).valid()) {
            $(".loaded").show();
            $(".loaded").html("Submitting...");
            var data = $('#newsletter-validate-detail').serialize();
            jQuery.ajax({
                url: "https://www.netmeds.com/newsletter/index/subscriber",
                type: 'POST',
                crossDomain: true,
                data: data,
                beforeSend: function() {
                    $('.loader').show();
                },
                success: function(response) {
                    if (response == 'Thank you for your subscription.') {
                        $(".loaded").hide();
                        $(".loaded").text('');
                        $('.load-out').show('');
                        $('.load-out').append("<span class='success-msg'>" + response + "</span>");

                    } else {
                        $(".loaded").hide();
                        $(".loaded").text('');
                        $('.load-out').show('');
                        $('.load-out').append("<span class='error-msg'>" + response + "</span>");
                    }
                }
            });
        }
        return false;
    });
    $.validator.addMethod(
    'Required-newsletter-email', function (value) {
        if(value) return true;
        else return false;
    }, 'Please enter your E-mail ID!');
    $.validator.addMethod('validate-newsletter-emailid',function(value,elem){
      return /^([a-z0-9,!\#\$%&'\*\+\/=\?\^_`\{\|\}~-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z0-9,!\#\$%&'\*\+\/=\?\^_`\{\|\}~-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*@([a-z0-9-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z0-9-]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*\.(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]){2,})$/i.test(value);
    }, 'Please enter a valid E-mail ID!');
});
