//add function for hit log api
function genrate_log(api_url,start_time,complete_time,time_taken,modules,submodules,request_payload,xhr=''){

    if(xhr != ''){ 
        if(typeof xhr.responseJSON !== undefined){
            var responseJSON = xhr.responseJSON;
        }else{
            var responseJSON = 'N/A';
        }
        var statusCode = xhr.status;
    }else{
        var statusCode = 'N/A';
