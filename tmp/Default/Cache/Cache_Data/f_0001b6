    $(document).on('click','#search_request',function(event){
        if($("#search_product_request").length == 0){
            $.ajax({
                type: 'GET',
                url: "https://www.netmeds.com/catalogsearch/productrequest",
                start_time: new Date().getTime(),
                success:function(response){
                    $("body").append(response);
                    $("#search_product_request").modal("show");
                },
                error:function(xhr, status, error){
                    var time_taken = new Date().getTime() - this.start_time;
                    genrate_log(this.url,this.start_time,new Date().getTime(),time_taken,'footer scripts','productrequest',"get",xhr); 
                }
            });
