    var totalCartBarpdp = $("#totalCartBarpdp").val();
    if(totalCartBarpdp === 1 || totalCartBarpdp === '1'){
        pdptotalCartBar();
        function pdptotalCartBar(totalCartval=''){
            var cart_info = localStorage.getItem('cart_info');
            cart_info = JSON.parse(cart_info);
            
            totalCartval = parseFloat(cart_info.cart.sub_total - cart_info.cart.total_savings).toFixed(2);
            
            let circularProgress = document.querySelector(".circular-progress"),
            progressValue = document.querySelector(".progress-value");

            var totalCart = Math.round((totalCartval/addItemAmount)*100);
            
            $("#cartValueInBar").html('Cart Total<br/><span>₹'+totalCartval+'</span>');
            
            let progressStartValue = 0,    
                progressEndValue = totalCart,    
                speed = 1;

            if(totalCartval >= Number(addItemAmount)){
                progressValue.style.color = `#31af30`
                $("#newBestPrice").addClass("offerUnlocked");
                if(offerUnlockedMsite === true){
                    $("#addItemText").html(offerUnlockedText);   
                }
            }else{
                progressValue.style.color = `#a7a7a7`
                $("#newBestPrice").removeClass("offerUnlocked");
                $("#addItemText").html(addItemText);
            }
                
            let progress = setInterval(() => {
                    progressStartValue++;

                    circularProgress.style.background = `conic-gradient(#31af30 ${progressStartValue * 3.6}deg, #ededed 0deg)`
                    if(progressStartValue == progressEndValue){
                        clearInterval(progress);
                    }    
            }, speed);
        }
    }
    

    function totalCartBar(totalCartval=''){
        var cart_info = localStorage.getItem('cart_info');
        cart_info = JSON.parse(cart_info);
        
        totalCartval = parseFloat(cart_info.cart.sub_total - cart_info.cart.total_savings).toFixed(2);
        
        let circularProgress = document.querySelector(".circular-progress"),
        progressValue = document.querySelector(".progress-value"),
        circularProgressPop = document.querySelector(".circular-progress-pop"),
        progressValuePop = document.querySelector(".progress-value-pop");

        var totalCart = Math.round((totalCartval/addItemAmount)*100);
        
        //$("#cartValueInBar").html('Cart Total<br/><span>₹'+totalCartval+'</span>');
        $(".cartValueInBarpop").html('Cart Total<br/><span>₹'+totalCartval+'</span>');
        //$(".circular-progress-pop").css({'background':'conic-gradient(rgb(49, 175, 48) 586.8deg, rgb(237, 237, 237) 0deg)'});
        
        let progressStartValue = 0,    
            progressEndValue = totalCart,    
            speed = 1;

        if(totalCartval >= Number(addItemAmount)){
            //progressValue.style.color = `#31af30`
            progressValuePop.style.color = `#31af30`
            //$("#newBestPrice").addClass("offerUnlocked");
            $(".newBestPricepop").addClass("offerUnlocked");
            $(".popuptext").addClass("offerUnlockedpopup");
            
            if(offerUnlockedMsite === true){
                $(".addItemTextpop").html(offerUnlockedText);
                //$("#addItemText").html(offerUnlockedText);   
            }
        }else{
            //progressValue.style.color = `#a7a7a7`
            progressValuePop.style.color = `#a7a7a7`
            //$("#newBestPrice").removeClass("offerUnlocked");
            $(".newBestPricepop").removeClass("offerUnlocked");
            $(".popuptext").removeClass("offerUnlockedpopup");
            //$("#addItemText").html(addItemText);
            $(".addItemTextpop").html(addItemText);
        }
            
        let progress = setInterval(() => {
            progressStartValue++;
            
            var color = 'conic-gradient(#31af30 '+progressStartValue * 3.6+'deg, #ededed 0deg)';
                $(".circular-progress-pop").css({'background':color});
            //progressValue.textContent = `${progressStartValue}%`
            //circularProgress.style.background = `conic-gradient(#31af30 ${progressStartValue * 3.6}deg, #ededed 0deg)`
            //circularProgressPop.style.background = `conic-gradient(#31af30 ${progressStartValue * 3.6}deg, #ededed 0deg)`
            if(progressStartValue == progressEndValue){
                clearInterval(progress);
            }    
        }, speed);
    }

